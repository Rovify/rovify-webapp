{
  "name": "rovify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "check-types": "tsc --noEmit --incremental",
    "dev": "next dev --turbopack",
    "format": "prettier --write . '!(node_modules|.next|contracts)/**/*'",
    "ipfs": "NEXT_PUBLIC_IPFS_BUILD=true npm run build && npx bgipfs upload config init -u https://upload.bgipfs.com && CID=$(npx bgipfs upload out | grep -o 'CID: [^ ]*' | cut -d' ' -f2) && [ ! -z \"$CID\" ] && echo 'üöÄ Upload complete! Your site is now available at: https://community.bgipfs.com/ipfs/'$CID || echo '‚ùå Upload failed'",
    "lint": "next lint",
    "serve": "next start",
    "start": "next dev",
    "vercel": "vercel --build-env ENABLE_EXPERIMENTAL_COREPACK=1 --build-env VERCEL_TELEMETRY_DISABLED=1",
    "vercel:login": "vercel login",
    "vercel:yolo": "vercel --build-env ENABLE_EXPERIMENTAL_COREPACK=1 --build-env NEXT_PUBLIC_IGNORE_BUILD_ERROR=true --build-env VERCEL_TELEMETRY_DISABLED=1",
    "chain": "cd hardhat && yarn chain",
    "deploy": "cd hardhat && yarn deploy",
    "deploy:local": "cd hardhat && yarn deploy --network localhost",
    "setup:debug": "yarn chain & yarn deploy:local & yarn dev",
    "verify:debug": "node verify-debug.js"
  },
  "dependencies": {
